#include <stdio.h>

//Entrada de dados.

int main (){

    int idade = 0;
    float peso = 19.50;



    printf("Digite uma idade: \n");
    scanf("%d", &idade);


    printf("Digite uma peso: \n");
    scanf("%f", &peso);


    printf("Idade informada: %d. \n", idade);
    printf("Ano informada: %f. \n", peso);
}
